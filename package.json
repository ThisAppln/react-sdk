    "@pega/dx-component-builder-sdk": "latest",
    "@pega/eslint-config": "^0.3.2",
    "@pega/prettier-config": "^0.3.2",
    "@pega/react-sdk-overrides": "file:pega-react-sdk-overrides-0.23.5.tgz",
